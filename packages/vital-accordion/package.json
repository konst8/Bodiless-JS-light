{
  "name": "@bodiless/vital-accordion",
  "version": "1.0.0-rc.9",
  "description": "Clean, designable Accordion component for VitalDS based projects",
  "author": "Lucas Varela <lvarelab@its.jnj.com>",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "sideEffects": false,
  "typings": "lib/index.d.ts",
  "files": [
    "/lib",
    "/bodiless.docs.json",
    "/tailwind.config.js",
    "/doc"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/johnsonandjohnson/bodiless-js.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc -p ./tsconfig.json && npm run copy",
    "build:watch": "npm run build -- --watch",
    "copy": "copyfiles -u 1 \"./src/*.css\" \"./lib/\"",
    "clean": "rimraf \"lib/*\" && rimraf tsconfig.tsbuildinfo",
    "lint": "eslint --cache --ext .js,.jsx,.ts,.tsx ./src"
  },
  "dependencies": {
    "@bodiless/components": "^1.0.0-rc.9",
    "@bodiless/core": "^1.0.0-rc.9",
    "@bodiless/fclasses": "^1.0.0-rc.9",
    "@bodiless/layouts": "^1.0.0-rc.9",
    "@bodiless/schema-org": "^1.0.0-rc.9",
    "@bodiless/vital-editors": "^1.0.0-rc.9",
    "@bodiless/vital-elements": "^1.0.0-rc.9",
    "lodash": "^4.17.19"
  },
  "peerDependencies": {
    "react": "^17.0.2"
  }
}
